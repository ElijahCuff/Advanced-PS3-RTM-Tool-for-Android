<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
	<item android:state_pressed="true" >
		<shape>
			<gradient 
				android:startColor="#292929" 
				android:endColor="#292929" 
				android:angle="270" />
			<stroke 
				android:width="2dp"
				android:color="#FDFDFD" />
			<corners 
				android:radius="0dp" />
		</shape>
	</item>
</selector>
